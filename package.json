{
  "name": "stock-quotes-slack",
  "version": "1.3.0",
  "description": "Micro serviço para consultar informações de um papel na Bolsa de Valores",
  "main": "index.js",
  "scripts": {
    "test": "xo",
    "start": "NODE_ENV=production micro -p ${PORT:-5000}"
  },
  "repository": "lagden/stock-quote-slack",
  "keywords": [
    "stock",
    "quote",
    "api"
  ],
  "author": "Thiago Lagden",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lagden/stock-quote/issues"
  },
  "engines": {
    "node": "8.6.0"
  },
  "dependencies": {
    "got": "7.1.0",
    "lagden-stock-quote": "1.1.1",
    "micro": "9.0.0"
  },
  "devDependencies": {
    "xo": "0.18.2"
  },
  "xo": {
    "semicolon": false,
    "esnext": true,
    "env": [
      "node",
      "es6"
    ],
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ]
    }
  }
}
